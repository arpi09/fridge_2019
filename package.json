{
  "name": "fridge",
  "version": "1.0.0",
  "description": "Fridge app",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix fridge",
    "start": "node server.js --ignore client",
    "server": "node_modules/.bin/nodemon app.js",
    "client": "npm start --prefix fridge",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "heroku-postbuild": "cd fridge && npm install && npm run build"
  },
