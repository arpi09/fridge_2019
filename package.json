    "concurrently": "^4.1.2",
    "create-react-app": "^3.4.1",
    "crypto-js": "^3.3.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "heroku": "^7.43.2",
    "jsonwebtoken": "^8.5.1",
    "material-table": "^1.69.0",
    "moment": "^2.29.0",
    "mui-datatables": "^2.15.0",
    "nodemon": "^1.19.4",
    "react": "^16.13.1",
    "react-animations": "^1.0.0",
    "react-charts": "^2.0.0-beta.6",
